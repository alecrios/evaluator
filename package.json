		},
		"publish": {
			"provider": "github"
		}
	},
	"dependencies": {
		"electron-is-dev": "^1.1.0",
		"electron-updater": "^4.1.2",
		"evaluator.js": "^3.0.1"
	},
	"devDependencies": {
		"electron": "^4.0.5",
		"electron-builder": "^20.44.4",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.2.0",
		"eslint-plugin-import": "^2.18.2"
	}
