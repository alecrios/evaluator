{
	"name": "evaluator",
	"version": "0.9.3",
	"author": "Alec Rios",
	"description": "A powerful calculator on standby.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/alecrios/evaluator.git"
	},
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build": "build",
		"ship": "build -p always"
	},
	"build": {
		"appId": "com.electron.evaluator",
		"productName": "Evaluator",
